<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="padam.keyrecapp">

    <application
        android:name="padam.keyrecapp.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="padam.keyrecapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="padam.keyrecapp.PressureTestActivity" />
        <activity
            android:name="padam.keyrecapp.EditTextKeyboardTestActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>